//write a program to print multiplication table using java method in java (best way).
//coded By Abhishek Dadasaheb Shelar.

package com.company;

import java.util.Scanner;

class Main{

    static void multiplication(int x){
        for (int i = 1; i <=10 ; i++) {
            System.out.format("%d X %d = %d\n",x,i, x*i);
        }
    }
    public static void main(String[] args) {

        Scanner s=new Scanner(System.in);
        System.out.println("Enter any value:");
        int num=s.nextInt();
        multiplication(num);
    }
}
