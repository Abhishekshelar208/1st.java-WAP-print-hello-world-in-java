/*
Write a program to print following pattern....
* * * * *
* * * *
* * *
* *
*
*/
//coded BY Abhishek Dadasaheb shelar.
//java practice set 5.
package com.company;

class Main{
    public static void main(String[] args) {
        for (int i = 1; i <=5; i++) {
            System.out.print("* ");
            for (int j = 5; j >i ; j--) {
                System.out.print("* ");

            }
            System.out.println();
        }
    }
}
