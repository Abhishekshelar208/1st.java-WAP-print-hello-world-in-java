//write a program to find greater number between three numbers using nested if else in java.
//coded BY Abhishek Dadasaheb Shelar.

package com.company;

import java.util.Scanner;

class Main{
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        System.out.println("Enter three number:");
        int a=s.nextInt();
        int b=s.nextInt();
        int c=s.nextInt();

        if(a>b){
            if(a>c){
                System.out.println("Greater is:"+ a);
            }
            else{
                System.out.println("Greater is:"+ c);
            }
        }
        else{
           if(b>c){
               System.out.println("Greater is:"+ b);
           }
           else{
               System.out.println("Greater is:"+ c);
           }
        }
    }
}


