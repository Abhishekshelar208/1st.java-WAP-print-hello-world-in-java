package com.company;

import jdk.swing.interop.SwingInterOpUtils;

class Employee{
    int salary;
    String name;

    public int getSalary(){
        return salary;
    }
    public void getName(){
        System.out.println(name);
    }
    public void setName(String n){
        name = n;
        System.out.println(name);
    }
}

 public class Main{
    public static void main(String[] args) {
       Employee abhi = new Employee();

       abhi.name = "Abhishek";
       abhi.salary=233;
        abhi.getSalary();
       abhi.getName();

    }
}
