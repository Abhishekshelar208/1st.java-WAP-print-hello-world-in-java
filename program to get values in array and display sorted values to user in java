//write a program to get values in array and display sorted values to user in java
//Coded by Abhishek Dadasaheb Shelar.

package com.company;

import java.util.Arrays;
import java.util.Scanner;

class Main
{
    public static void main(String[] sdr)
    {
        int arr[]=new int[5];
        System.out.print("Enter any 5 values in array");
        Scanner s=new Scanner(System.in);
        for(int i=0; i<5; i++)
        {
            arr[i]=s.nextInt();
        }
        Arrays.sort(arr);
        System.out.print("your array values are:");
        for(int b: arr)
        {
            System.out.print(b+" ");
        }
    }
}
