//write a program to print pattern like this:
/*

 * * * *
 * * *
 * *
 *

 */
//coded By Abhishek Dadasaheb Shelar.

package com.company;

class Main{
    static void pattern(){
        for (int i = 0; i < 4; i++) {
            for (int j = 4; j > i; j--) {
                System.out.print("* ");
            }
            System.out.println();
        }
    }
    public static void main(String[] args) {
        System.out.println("Printing your pattern");
        pattern();
    }
}
