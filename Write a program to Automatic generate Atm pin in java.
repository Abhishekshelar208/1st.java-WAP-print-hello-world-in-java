//Write a program to Automatic generate Atm pin in java.
//coded BY Abhishek Dadasaheb Shelar.

package com.company;

import java.util.Scanner;

class Main{
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int pin=(int)(Math.random()*10000);

        System.out.println("This is your ATM Pin:");
        int pass=s.nextInt();

        if(pass==pin){
            System.out.println("Matched");
        }else{
            System.out.println("Not matched");
            System.out.println("because our pin is: "+pin);
        }


    }
}












